{
  "name": "@prometheusresearch/react-ui",
  "version": "0.22.3",
  "description": "Configurable and styleable UI components",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "topNav.js"
  ],
  "scripts": {
    "test": "make lint test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "prometheusresearch",
    "env": {
      "mocha": true
    },
    "globals": {
      "ReactClass": true,
      "React$Element": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prometheusresearch/react-ui.git"
  },
  "keywords": [
    "react-component"
  ],
  "author": "Prometheus Research, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prometheusresearch/react-ui/issues"
  },
  "homepage": "https://github.com/prometheusresearch/react-ui#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.3.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-jest": "^17.0.0",
    "babel-preset-prometheusresearch": "^0.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^2.10.2",
    "eslint-config-prometheusresearch": "^0.2.0",
    "eslint-plugin-react": "^6.1.2",
    "flow-bin": "^0.37.0",
    "flow-coverage-report": "^0.2.0",
    "indent-string": "^2.1.0",
    "jest-cli": "^17.0.0",
    "normalize.css": "^4.1.1",
    "power-assert": "^1.4.1",
    "react": "^15.3.1",
    "react-scripts": "^0.8.4",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-icons": "^2.1.0",
    "react-test-renderer": "^15.3.1",
    "storyshots": "^3.2.0"
  },
  "dependencies": {
    "color-js": "^1.0.3",
    "debounce": "^1.0.0",
    "invariant": "^2.2.1",
    "react-stylesheet": "^2.0.0"
  }
}
